function lrestatus()    //录像颜色
{
 var flag=DHiMPlayer.GetRecordState();
 if(flag==0)
 {
 Currentid=document.all("y_1");
	Currentid.bgColor="#FF0033"
 }
 else
 {
  Currentid=document.all("y_1");
	Currentid.bgColor="#8CC6FF"
 }
 }
