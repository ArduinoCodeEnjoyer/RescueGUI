function load1()
{
	if (navigator.appName.indexOf("Microsoft Internet Explorer") == -1)
	{
		document.getElementById("addport").style.display = "none";
		document.getElementById("videoshade").style.display = "none";
		document.getElementById("md").style.display = "none";
	}

	if (lancode=='0')
	{
		document.getElementById("tmall").style.display = "";
		document.getElementById("amazon").style.display = "none";
		document.getElementById("cloud").style.display = "";
	}
	else
	{
		document.getElementById("tmall").style.display = "none";
		document.getElementById("amazon").style.display = "none";
		document.getElementById("cloud").style.display = "none";
	}
}

